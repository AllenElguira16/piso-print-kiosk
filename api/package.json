{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "dev": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "typeorm": "tsed typeorm"
  },
  "dependencies": {
    "@tsed/ajv": "^6.73.1",
    "@tsed/common": "^6.73.1",
    "@tsed/core": "^6.73.1",
    "@tsed/di": "^6.73.1",
    "@tsed/exceptions": "^6.73.1",
    "@tsed/json-mapper": "^6.73.1",
    "@tsed/platform-express": "^6.73.1",
    "@tsed/schema": "^6.73.1",
    "@tsed/socketio": "^6.73.1",
    "@tsed/typeorm": "^6.73.1",
    "ajv": "^8.6.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "socket.io": "^4.2.0",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.11.6",
    "@tsed/cli-plugin-typeorm": "3.11.6",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default"
  }
}
